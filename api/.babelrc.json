{
  "presets": [
    [
      "@babel/preset-env",
      {
        "modules": false,
        "targets": {
          "node": "current"
        }
      }
    ],
    "@babel/preset-typescript"
  ],
  "plugins": [
    "add-import-extension",
    [
      "module-resolver",
      {
        "alias": {
          ":bookcars-types": "../packages/bookcars-types"
        }
      }
    ]
  ]
}
