{
  "cli": {
    "version": ">= 0.53.0"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "android": {
        "image": "default",
        "gradleCommand": ":app:assembleDebug"
      },
      "ios": {
        "image": "default",
        "buildConfiguration": "Debug"
      }
    },
    "preview": {
      "distribution": "internal"
    },
    "production": {
      "android": {
        "image": "default",
        "buildType": "apk"
      },
      "ios": {
        "image": "default"
      }
    }
  },
  "submit": {
    "production": {}
  }
}